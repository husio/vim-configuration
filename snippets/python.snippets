snippet #!
	#!/usr/bin/env python
	# -*- coding: `&enc`

snippet #enc
	# -*- coding: `&enc`
	
snippet __info__
	__version__ = '$1'
	__author__ = 'Piotr Husiatynski <phusiatynski@gmail.com>'
	__revision__ = '$Id$'
	$2

snippet ifname
	def main():
		${1:pass}
		$2
	
	if __name__ == '__main__':
		main()

snippet trace
	import ipdb; ipdb.set_trace()
